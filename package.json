{
  "name": "serverless-discord-client",
  "version": "0.5.0",
  "license": "MIT",
  "main": "main/electron/index.js",
  "author": {
    "name": "CodingButter"
  },
  "description": "Serverless Discord",
  "keywords": [
    "vite",
    "react",
    "electron",
    "typescript"
  ],
  "repository": {
    "url": "https://github.com/CodingButter/serverless-chat"
  },
  "productName": "Serverless Discord Client",
  "scripts": {
    "dev": "concurrently \"yarn dev:vite\" \" yarn dev:electron\"",
    "dev:vite": "vite",
    "dev:electron": "npm run build:electron && electron .",
    "build": "npm run build:vite && npm run build:electron",
    "build:vite": "vite build",
    "build:electron": "tsc -p electron",
    "dist": "npm run build && electron-builder",
    "pack": "npm run build && electron-builder --dir",
    "clean": "rimraf dist main src/out",
    "type-check": "tsc",
    "lint": "eslint . --ext js,jsx,ts,tsx",
    "lint:fix": "eslint . --ext js,jsx,ts,tsx --fix",
    "knex": "knex --knexfile ./main/knexfile.js ",
    "migrate:latest": "npm run knex migrate:latest",
    "migrate:rollback": "npm run knex migrate:rollback",
    "migrate:make": "npm run knex migrate:make",
    "seed:run": "npm run knex seed:run",
    "seed:make": "npm run knex seed:make"
  },
  "dependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.14",
    "@types/isomorphic-fetch": "^0.0.36",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/nedb": "^1.8.12",
    "@types/peerjs": "^1.1.0",
    "@vitejs/plugin-react": "^1.2.0",
    "classnames": "^2.3.2",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "electron-debug": "^3.2.0",
    "electron-is-dev": "^2.0.0",
    "electron-store": "^8.1.0",
    "express": "^4.18.2",
    "fast-md5": "^1.0.3",
    "graphql": "^16.6.0",
    "jsonwebtoken": "^8.5.1",
    "knex": "^2.3.0",
    "ms": "^2.1.3",
    "my-react-snackbar": "^1.0.4",
    "nedb": "^1.8.0",
    "peerjs": "^1.4.7",
    "qrcode.react": "^3.1.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-drag-drop-files": "^2.3.7",
    "react-icons": "^4.4.0",
    "react-router-dom": "^6.4.1",
    "react-spring": "^9.5.5",
    "short-hash": "^1.0.0",
    "sqlite3": "^5.1.2",
    "suspend-react": "^0.0.8",
    "uuid": "^9.0.0",
    "uuid-by-string": "^4.0.0",
    "vite": "^2.8.6"
  },
  "devDependencies": {
    "@types/graphql": "^14.5.0",
    "@types/node": "^17.0.22",
    "@types/react": "^17.0.41",
    "@types/react-dom": "^17.0.14",
    "@typescript-eslint/eslint-plugin": "5.16.0",
    "@typescript-eslint/parser": "5.16.0",
    "autoprefixer": "^10.4.4",
    "concurrently": "^7.0.0",
    "electron": "^17.1.2",
    "electron-builder": "^22.14.13",
    "electron-react-devtools": "firejune/electron-react-devtools",
    "eslint": "8.11.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "postcss": "^8.4.12",
    "prettier": "2.6.0",
    "tailwindcss": "^3.0.23",
    "typescript": "^4.6.2"
  },
  "build": {
    "appId": "com.codingbutter.serverless-discord-client",
    "asar": false,
    "files": [
      "main",
      "src/out"
    ],
    "directories": {
      "buildResources": "resources"
    }
  }
}
